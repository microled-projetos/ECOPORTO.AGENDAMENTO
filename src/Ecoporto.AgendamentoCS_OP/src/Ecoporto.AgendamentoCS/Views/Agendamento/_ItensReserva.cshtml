@model  Ecoporto.AgendamentoCS.Models.ViewModels.AgendamentoViewModel

<div class="row">
    <div class="form-group col-md-11 mb-1">
        @Html.LabelFor(model => model.ItemReservaId)
        @Html.DropDownListFor(c => c.ItemReservaId, new List<SelectListItem>(), string.Empty, new { @class = "form-control form-control-sm" })
    </div>
    <div class="form-group col-md-1 mb-1">
        @Html.LabelFor(model => model.QuantidadeItemReserva)
        @Html.EditorFor(model => model.QuantidadeItemReserva, new { htmlAttributes = new { @class = "form-control form-control-sm" } })
    </div>
</div>

<div class="row">
    <div class="form-group col-md-11">
        @Html.LabelFor(model => model.Chassis)
        @Html.TextAreaFor(model => model.Chassis, new { @class = "form-control form-control-sm", @rows="5", @readonly = ""  })

    </div>
    <div class="form-group col-md-1">
        <label for="btnAdicionarItemReserva">&nbsp;</label>
        <a id="btnAdicionarItemReserva" href="#" class="btn btn-sm btn-outline-info btn-block btn-as-block"><i class="fas fa-save"></i></a>
    </div>
</div>

@Html.Partial("_ItensReservaConsulta", Model.ItensReserva)